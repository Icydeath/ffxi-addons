<?xml version="1.1" ?>
<settings>
    <global>
        <default>ffxiclopedia</default>
        <sites>
            <ah>
                <redirect>ffxiah</redirect>
            </ah>
            <ahp>
                <redirect>ffxiahplayer</redirect>
            </ahp>
            <bg>
                <redirect>bg-wiki</redirect>
            </bg>
            <bg-wiki>
                <search>https://www.bg-wiki.com/bg/Special:Search?go=Go&amp;search=${term}</search>
            </bg-wiki>
            <bgwiki>
                <redirect>bg-wiki</redirect>
            </bgwiki>
            <db>
                <redirect>ffxidb</redirect>
            </db>
            <ffxi>
                <redirect>ffxiclopedia</redirect>
            </ffxi>
            <ffxiah>
                <item>http://www.ffxiah.com/item/${term}</item>
                <search>http://www.ffxiah.com/search/item?q=${term}</search>
            </ffxiah>
            <ffxiahp>
                <redirect>ffxiahplayer</redirect>
            </ffxiahp>
            <ffxiahplayer>
                <search>https://www.ffxiah.com/search/player?name=${term}</search>
            </ffxiahplayer>
            <ffxiclopedia>
                <search>http://ffxiclopedia.wikia.com/wiki/Special:Search?go=Go&amp;search=${term}</search>
            </ffxiclopedia>
            <ffxidb>
                <item>http://www.ffxidb.com/items/${term}</item>
                <search>http://www.ffxidb.com/search?q=${term}</search>
                <zone>http://www.ffxidb.com/zones/${term}</zone>
            </ffxidb>
            <google>
                <search>https://www.google.com/search?q=${term}</search>
            </google>
            <wikia>
                <redirect>ffxiclopedia</redirect>
            </wikia>
        </sites>
    </global>
</settings>
